{
  "@sys.entity.poetry-title:title": [
    "茅屋为秋风所破歌",
    "登高",
    "春望",
    "江南逢李龟年"
  ],
  "@sys.entity.poetry-content:content": [
    "俄顷风定云墨色",
    "秋天漠漠向昏黑",
    "无边落木萧萧下",
    "不尽长江滚滚来",
    "万里悲秋常作客",
    "百年多病独登台",
    "感时花溅泪",
    "恨别鸟惊心",
    "烽火连三月",
    "家书抵万金",
    "正是江南好风景",
    "落花时节又逢君"
  ],
  "@sys.entity.poetry-authorname:authorname": [
    "杜甫"
  ],
